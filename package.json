    "chart.js": "^2.7.1",
    "element-closest": "^2.0.2",
    "focus-within": "^2.0.0",
    "h": "^0.1.0",
    "headroom.js": "^0.9.4",
    "intersection-observer": "^0.5.0",
    "lodash.debounce": "^4.0.8",
    "menuspy": "^1.3.0",
    "normalize.css": "^8.0.0",
    "object-fit-images": "^3.2.3",
    "onresize": "^1.0.1",
    "pikaday": "^1.7.0",
    "smooth-scroll": "^14.2.1",
    "swiper": "^4.1.0"
  },
  "bundlesize": [
    {
      "path": "./dist/css/*.css",
