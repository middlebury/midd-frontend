{
  "name": "midd-frontend",
  "description": "Frontend assets for Drupal8 middlebury_theme",
  "version": "0.0.1",
  "scripts": {
    "start": "npm run dev",
    "dev": "gulp dev",
    "dev:saw": "gulp devSaw",
    "build": "NODE_ENV=production gulp build",
    "build:scripts": "NODE_ENV=production gulp scripts",
    "deploy": "gulp deploy",
    "deploy:gh": "npm run build && gh-pages -d dist -b master-dist",
    "now-build": "npm run build && mv dist public",
    "test": "npm run test:bundlesize",
    "test:visual": "percy snapshot dist",
    "test:bundlesize": "bundlesize",
    "lint": "eslint src/js/**",
    "format": "prettier --config ./.prettierrc --write \"src/**/*.{js,scss}\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/middlebury/midd-frontend.git"
  },
  "bugs": {
    "url": "https://github.com/middlebury/midd-frontend/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-transform-react-jsx": "^7.3.0",
    "@babel/preset-env": "^7.6.3",
    "babel-eslint": "^10.0.3",
    "browser-sync": "^2.26.7",
    "bundlesize": "^0.18.0",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.10",
    "del": "^5.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.6.0",
    "eslint-plugin-react": "^7.16.0",
    "gh-pages": "2.1.1",
    "gulp": "^4.0.2",
    "gulp-data": "^1.3.1",
    "gulp-dom": "^1.0.0",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^5.0.3",
    "gulp-notify": "^3.2.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^8.0.0",
    "gulp-prettify": "^0.5.0",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-size": "^3.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-svg-sprite": "^1.5.0",
    "gulp-svgo": "^2.1.1",
    "gulp-twig": "^1.2.0",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.15",
    "percy": "^0.19.6",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.18.2",
    "rollup": "^0.67.0",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-filesize": "^6.0.1",
    "rollup-plugin-ignore": "^1.0.4",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-sizes": "^0.5.1",
    "rollup-plugin-uglify": "^6.0.0",
    "sort-css-media-queries": "^1.4.3",
    "yargs": "^14.2.0"
  },
  "dependencies": {
    "anchor-js": "^4.2.0",
    "animejs": "^2.2.0",
    "chart.js": "^2.9.1",
    "element-closest": "^3.0.2",
    "enquire.js": "^2.1.6",
    "focus-within": "^2.0.0",
    "headroom.js": "^0.9.4",
    "iframe-resizer": "^4.1.1",
    "intersection-observer": "^0.5.1",
    "lodash.debounce": "^4.0.8",
    "lozad": "^1.9.0",
    "mdn-polyfills": "^5.16.0",
    "micromodal": "git+https://git@github.com/zebapy/micromodal.git",
    "normalize.css": "^8.0.0",
    "object-fit-images": "^3.2.4",
    "onresize": "^1.0.1",
    "pikaday": "^1.7.0",
    "polyfill-nodelist-foreach": "^1.0.1",
    "preact": "^8.3.1",
    "stickybits": "^3.5.6",
    "swiper": "^4.5.0"
  },
  "bundlesize": [
    {
      "path": "./dist/css/*.css",
      "maxSize": "24 kB"
    },
    {
      "path": "./dist/js/bundle.js",
      "maxSize": "53 kB"
    }
  ]
}
