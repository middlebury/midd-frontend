{
  "name": "midd-frontend",
  "description": "Frontend assets for Drupal8 middlebury_theme",
  "version": "0.0.1",
  "scripts": {
    "start": "npm run dev",
    "dev": "gulp dev",
    "dev:saw": "gulp devSaw",
    "build": "NODE_ENV=production gulp build",
    "build:scripts": "NODE_ENV=production gulp scripts",
    "build:slate": "BUILD_SLATE=true npm run build",
    "wrapper": "npm run build && gulp zip",
    "deploy": "gulp deploy",
    "deploy:gh": "npm run build && gh-pages -d dist -b master-dist",
    "test": "npm run test:bundlesize",
    "test:visual": "percy snapshot dist",
    "test:bundlesize": "bundlesize",
    "lint": "eslint src/js/**",
    "format": "prettier --config ./.prettierrc --write \"src/**/*.{js,scss}\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/middlebury/midd-frontend.git"
  },
  "bugs": {
    "url": "https://github.com/middlebury/midd-frontend/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-transform-react-jsx": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "babel-eslint": "^10.0.1",
    "beeper": "^1.1.1",
    "browser-sync": "^2.26.7",
    "bundlesize": "^0.17.2",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.10",
    "del": "^3.0.0",
    "dotenv": "^6.1.0",
    "eslint": "^5.4.0",
    "eslint-plugin-react": "^7.11.1",
    "gh-pages": "2.0.1",
    "gulp": "^4.0.0",
    "gulp-data": "^1.3.1",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^5.0.3",
    "gulp-notify": "^3.2.0",
    "gulp-plumber": "^1.2.0",
    "gulp-postcss": "^8.0.0",
    "gulp-prettify": "^0.5.0",
    "gulp-rename": "^1.4.0",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-size": "^3.0.0",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-twig": "^1.2.0",
    "gulp-zip": "^4.2.0",
    "js-yaml": "^3.13.1",
    "lodash": "^4.17.11",
    "percy": "^0.7.0",
    "postcss-preset-env": "^6.3.0",
    "prettier": "^1.14.3",
    "rollup": "^0.67.0",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-filesize": "^6.0.1",
    "rollup-plugin-ignore": "^1.0.4",
    "rollup-plugin-node-resolve": "^3.4.0",
    "rollup-plugin-sizes": "^0.5.1",
    "rollup-plugin-uglify": "^6.0.0",
    "sort-css-media-queries": "^1.4.1",
    "yargs": "^12.0.2"
  },
  "dependencies": {
    "anchor-js": "^4.1.1",
    "animejs": "^2.2.0",
    "chart.js": "^2.7.3",
    "element-closest": "^2.0.2",
    "enquire.js": "^2.1.6",
    "focus-within": "^2.0.0",
    "h": "^0.1.0",
    "headroom.js": "^0.9.4",
    "intersection-observer": "^0.5.1",
    "lodash.debounce": "^4.0.8",
    "lozad": "^1.9.0",
    "mdn-polyfills": "^5.16.0",
    "micromodal": "git+https://git@github.com/zebapy/micromodal.git",
    "middlebury-design-system": "^0.12.0",
    "normalize.css": "^8.0.0",
    "object-fit-images": "^3.2.4",
    "onresize": "^1.0.1",
    "pikaday": "^1.7.0",
    "polyfill-nodelist-foreach": "^1.0.1",
    "preact": "^8.3.1",
    "stickybits": "^3.5.6",
    "swiper": "^4.5.0"
  },
  "bundlesize": [
    {
      "path": "./dist/css/*.css",
      "maxSize": "24 kB"
    },
    {
      "path": "./dist/js/bundle.js",
      "maxSize": "51 kB"
    }
  ]
}
