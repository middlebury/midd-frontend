{
  "name": "midd-frontend",
  "description": "Frontend assets for Drupal8 middlebury_theme",
  "version": "0.0.1",
  "scripts": {
    "start": "npm run dev",
    "dev": "gulp dev",
    "dev:saw": "gulp devSaw",
    "build": "NODE_ENV=production gulp build",
    "build:scripts": "NODE_ENV=production gulp scripts",
    "build:styles": "NODE_ENV=production gulp styles",
    "build:icons": "NODE_ENV=production gulp icons",
    "deploy": "gulp deploy",
    "deploy:gh": "npm run build && gh-pages -d dist -b master-dist",
    "now-build": "npm run build && mv dist public",
    "test": "npm run test:bundlesize",
    "test:visual": "percy snapshot dist --snapshot-files \"*.html\"",
    "test:bundlesize": "bundlesize",
    "lint": "npm run lint:js && npm run lint:styles",
    "lint:js": "eslint src/js/**",
    "lint:styles": "stylelint src/scss/*",
    "force-resolutions": "npx npm-force-resolutions",
    "format": "prettier --config ./.prettierrc --write \"src/**/*.{js,scss}\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/middlebury/midd-frontend.git"
  },
  "bugs": {
    "url": "https://github.com/middlebury/midd-frontend/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/plugin-transform-react-jsx": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@rollup/plugin-babel": "^5.0.4",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "autoprefixer": "^9.8.4",
    "babel-eslint": "^10.1.0",
    "browser-sync": "^2.26.7",
    "bundlesize": "^0.18.0",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.10",
    "del": "^5.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-config-xo": "^0.31.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "gh-pages": "^3.1.0",
    "gulp": "^4.0.2",
    "gulp-data": "^1.3.1",
    "gulp-dom": "^1.0.0",
    "gulp-eslint": "^6.0.0",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^7.1.0",
    "gulp-notify": "^3.2.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^8.0.0",
    "gulp-prettify": "^0.5.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.1.0",
    "gulp-size": "^3.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-stylelint": "^13.0.0",
    "gulp-svg-sprite": "^1.5.0",
    "gulp-svgo": "^2.2.1",
    "gulp-twig": "^1.2.0",
    "js-yaml": "^3.14.0",
    "percy": "^0.26.9",
    "prettier": "^1.19.1",
    "prettier-plugin-twig-melody": "^0.4.6",
    "rollup": "^2.18.0",
    "rollup-plugin-filesize": "^9.0.1",
    "rollup-plugin-sizes": "^1.0.2",
    "rollup-plugin-uglify": "^6.0.4",
    "sort-css-media-queries": "^1.5.0",
    "stylelint": "^13.6.1",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-xo-scss": "^0.12.0",
    "stylelint-prettier": "^1.1.2",
    "yargs": "^15.3.1"
  },
  "dependencies": {
    "anchor-js": "^4.2.2",
    "animejs": "^3.2.0",
    "chart.js": "^2.9.3",
    "focus-within": "^3.0.2",
    "headroom.js": "^0.11.0",
    "iframe-resizer": "^4.2.11",
    "intersection-observer": "^0.11.0",
    "lodash": "^4.17.15",
    "lozad": "^1.15.0",
    "mdn-polyfills": "^5.20.0",
    "medium-zoom": "^1.0.5",
    "micromodal": "git+https://git@github.com/zebapy/micromodal.git",
    "normalize.css": "^8.0.1",
    "object-fit-images": "^3.2.4",
    "pikaday": "^1.8.0",
    "preact": "^10.4.4",
    "stickybits": "^3.7.7",
    "swiper": "^4.5.0"
  },
  "bundlesize": [
    {
      "path": "./dist/css/*.css",
      "maxSize": "25 kB"
    },
    {
      "path": "./dist/js/bundle.js",
      "maxSize": "55 kB"
    }
  ],
  "resolutions": {
    "minimist": "^1.2.2",
    "yargs-parser": "^18.1.2",
    "http-proxy": "^1.18.1"
  }
}
