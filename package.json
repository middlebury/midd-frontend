{
  "name": "midd-frontend",
  "description": "Frontend assets for Drupal8 middlebury_theme",
  "version": "0.0.1",
  "scripts": {
    "start": "npm run dev",
    "dev": "gulp dev",
    "dev:saw": "gulp devSaw",
    "build": "NODE_ENV=production gulp build",
    "build:scripts": "NODE_ENV=production gulp scripts",
    "build:icons": "NODE_ENV=production gulp icons",
    "deploy": "gulp deploy",
    "deploy:gh": "npm run build && gh-pages -d dist -b master-dist",
    "now-build": "npm run build && mv dist public",
    "test": "npm run test:bundlesize",
    "test:visual": "percy snapshot dist",
    "test:bundlesize": "bundlesize",
    "lint": "npm run lint:js && npm run lint:styles",
    "lint:js": "eslint src/js/**",
    "lint:styles": "stylelint src/scss/*",
    "format": "prettier --config ./.prettierrc --write \"src/**/*.{js,scss}\""
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/middlebury/midd-frontend.git"
  },
  "bugs": {
    "url": "https://github.com/middlebury/midd-frontend/issues"
  },
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/plugin-transform-react-jsx": "^7.7.7",
    "@babel/preset-env": "^7.7.7",
    "babel-eslint": "^10.0.3",
    "browser-sync": "^2.26.7",
    "bundlesize": "^0.18.0",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.10",
    "del": "^5.1.0",
    "dotenv": "^8.2.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-xo": "^0.27.2",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.17.0",
    "gh-pages": "2.2.0",
    "gulp": "^4.0.2",
    "gulp-data": "^1.3.1",
    "gulp-dom": "^1.0.0",
    "gulp-eslint": "^6.0.0",
    "gulp-if": "^3.0.0",
    "gulp-imagemin": "^6.2.0",
    "gulp-notify": "^3.2.0",
    "gulp-plumber": "^1.2.1",
    "gulp-postcss": "^8.0.0",
    "gulp-prettify": "^0.5.0",
    "gulp-rename": "^2.0.0",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-size": "^3.0.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-stylelint": "^11.0.0",
    "gulp-svg-sprite": "^1.5.0",
    "gulp-svgo": "^2.1.1",
    "gulp-twig": "^1.2.0",
    "js-yaml": "^3.13.1",
    "percy": "^0.20.6",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^1.19.1",
    "rollup": "^1.28.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-filesize": "^6.2.1",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-sizes": "^1.0.1",
    "rollup-plugin-uglify": "^6.0.4",
    "sort-css-media-queries": "^1.4.3",
    "stylelint": "^12.0.1",
    "stylelint-config-prettier": "^8.0.0",
    "stylelint-config-xo-scss": "^0.11.0",
    "stylelint-prettier": "^1.1.2",
    "yargs": "^14.2.0"
  },
  "dependencies": {
    "anchor-js": "^4.2.0",
    "animejs": "^3.1.0",
    "chart.js": "^2.9.2",
    "element-closest": "^3.0.2",
    "focus-within": "^3.0.2",
    "headroom.js": "^0.10.3",
    "iframe-resizer": "^4.2.3",
    "intersection-observer": "^0.7.0",
    "lodash": "^4.17.15",
    "lozad": "^1.14.0",
    "mdn-polyfills": "^5.19.0",
    "micromodal": "git+https://git@github.com/zebapy/micromodal.git",
    "normalize.css": "^8.0.1",
    "object-fit-images": "^3.2.4",
    "onresize": "^1.0.1",
    "pikaday": "^1.8.0",
    "polyfill-nodelist-foreach": "^1.0.1",
    "preact": "^10.0.4",
    "stickybits": "^3.6.7",
    "swiper": "^4.5.0"
  },
  "bundlesize": [
    {
      "path": "./dist/css/*.css",
      "maxSize": "24 kB"
    },
    {
      "path": "./dist/js/bundle.js",
      "maxSize": "53 kB"
    }
  ]
}
