{
  "name": "midd-frontend",
  "description": "Frontend assets for Drupal8 middlebury_theme",
  "version": "0.0.1",
  "scripts": {
    "start": "gulp",
    "dev": "gulp dev",
    "build": "gulp build --production",
    "build:icons": "gulp icons && gulp copy:icons",
    "wrapper": "npm run build && gulp bundle:markup",
    "deploy": "gulp deploy --production",
    "test": "bundlesize",
    "lint": "eslint src/js/**"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/middlebury/midd-frontend.git"
  },
  "bugs": {
    "url": "https://github.com/middlebury/midd-frontend/issues"
  },
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^9.0.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "beeper": "^1.1.1",
    "browser-sync": "^2.23.6",
    "bundlesize": "^0.17.0",
    "css-mqpacker": "^7.0.0",
    "cssnano": "^4.1.0",
    "del": "^2.2.2",
    "eslint": "^5.4.0",
    "gulp": "^3.9.1",
    "gulp-csscomb-lint": "^0.0.2",
    "gulp-data": "^1.2.1",
    "gulp-if": "^2.0.2",
    "gulp-imagemin": "^3.2.0",
    "gulp-notify": "^3.0.0",
    "gulp-plumber": "^1.1.0",
    "gulp-postcss": "^8.0.0",
    "gulp-prettify": "^0.5.0",
    "gulp-rename": "^1.2.2",
