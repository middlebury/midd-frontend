{% extends 'layout.twig' %}

{% set theme = 'wrmc' %}

{% block layout_header %}
  {% include 'partials/wrmc-header.twig' %}
  {% include 'paragraphs/wrmc-status-bar.twig' %}
{% endblock %}

{% block page %}
  <div class="container">
    <div class="row">
      <div class="paragraphs paragraphs--wide">
      {% block content %}{% endblock %}
      </div>
    </div>
  </div>
{% endblock %}

{% block layout_footer %}
  {% include 'partials/wrmc-footer.twig' %}
{% endblock %}