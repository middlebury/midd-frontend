{% extends "anchor-layout.twig" %}

{% set is_front = true %}
{% set page_title = 'Library' %}

{% block layout_header %}
  {% include 'partials/midd-header.twig' %}
  {% embed 'partials/anchor-header.twig' %}
    {% block anchor_header_content %}
      {% include 'partials/office-hours.twig' %}
    {% endblock %}
  {% endembed %}
{% endblock %}

{% block page %}
  {% embed 'partials/top-level-layout.twig' %}
    {% block paragraphs %}
      {% include 'paragraphs/lib-search.twig' %}
      {% include 'paragraphs/text-short.twig' %}
      {% include 'paragraphs/themed-panel.twig' %}
    {% endblock %}
  {% endembed %}
{% endblock %}

{% block scripts %}
  <script src="js/main.bundle.js" async></script>
  {% if (not env.production) or (env.vercel) %}
    <script src="//middlebury.libanswers.com/load_chat.php?hash=7799596699468240cefaabd896f80a52"></script>
  {% endif %}
{% endblock %}
