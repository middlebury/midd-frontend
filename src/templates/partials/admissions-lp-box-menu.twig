<div class="paragraph--expand paragraph--pull-up">
  <nav class="box-menu box-menu--admissions-lp border-dual-shadow__box " aria-labelledby="midd-box-menu-label">
    <h2 class="box-menu__title" id="midd-box-menu-label">Useful Links</h2>
    <ul class="box-menu__list">
      {% for item in items %}
        <li class="box-menu__item">
          <a href="" class="box-menu__link{% if item.active %} box-menu__link--active{% endif %}">
            {{item.text}}
          </a>
        </li>
      {% endfor %}
    </ul>
  </nav>
</div>