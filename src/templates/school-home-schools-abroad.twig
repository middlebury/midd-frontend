{% extends 'school-home.twig' %}

{% set theme = 'sa' %}
{% set is_front = true %}

{% block content %}
  {% include 'paragraphs/mega-cta.twig' with {
    pin: 'left-bottom',
    hero: true,
    caption: 'Lorem ipsum dolor sit amet consectetur adipisicing elit.'
  } %}

  {% include 'paragraphs/school-explorer.twig' %}

  {% include 'paragraphs/media-group.twig' with {
    media_type: 'image'
  } %}

  {% include 'paragraphs/video-group.twig' %}

  {% include 'paragraphs/themed-panel.twig' with {
    main_title: true,
    main_article: true,
    main_article_cta: true,
    main_article_title_link: true,
    subarticles: 3,
  } %}

  {% include 'paragraphs/spotlight.twig' %}

  {% include 'paragraphs/card-carousel.twig' with {
    colors: true,
    randomize: not env.test
  } %}

  {% include 'paragraphs/text-short.twig' %}

  {% include 'paragraphs/video.twig' %}

  {% include 'paragraphs/announcement.twig' %}

  {% include 'paragraphs/mega-cta.twig' %}

  {% include 'paragraphs/cta-group.twig' %}
{% endblock %}
