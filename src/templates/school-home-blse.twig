{% extends "school-home.twig" %}

{% set theme = 'blse' %}
{% set is_front = true %}

{% block layout_header %}
  {% embed 'partials/school-header.twig' %}
    {% block school_logo %}
      <img
        src="images/blse-logo.svg"
        srcset="images/blse-logo.svg 180w"
        sizes="(min-width: 1280px) 180px, 120px"
        alt="Logo for Middlebury Bread Loaf School of English"
      />
      <span class="sr-only">Middlebury Bread Loaf School of English</span>
    {% endblock %}
  {% endembed %}
{% endblock %}

{% block content %}

homepage

{% endblock %}
